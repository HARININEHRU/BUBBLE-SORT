#include<stdio.h>
#include<conio.h>
int findsmall(int a[],int,int);
int main()
{
int a[10];
int i,j,c,n;
clrscr();
printf("enter the value of n");
scanf("%d",&n);
for(i=0;i<n;i++)
{printf("enter the element:");
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
j=findsmall(a,i,n);
c=a[i];
a[i]=a[j];
a[j]=c;
}
for(i=0;i<n;i++)
printf("%d",a[i]);
getch();
return 0;
}
int findsmall(int a[],int i,int n)
{
int j,small;
j=i;
small=a[j];
for(j=i+1;j<n;j++)
{
if(a[j]<small)
{
small=a[j];
i=j;
} }
return i;
}
